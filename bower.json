{
  "name": "butterfly",
  "version": "0.0.2",
  "authors": [
    "Justin Yip <justin.yip@foxmail.com>"
  ],
  "description": "Butterfly Mobile Web Framework",
  "main": "dist/js/butterfly.js",
  "keywords": ["HTML5", "Mobile", "Web"],
  "license": "MIT",
  "homepage": "http://yezhiming.github.io/butterfly",
  "ignore": [
    "node_modules",
    "bower_components",
    "tests",
    "examples"
  ],
  "dependencies": {
    "requirejs": "*",
    "requirejs-text": "*",
    "requirejs-i18n": "*",
    "requirejs-domready": "*",
    "fastclick": "*",
    "iscroll": "*",
    "jquery": "*",
    "underscore": "*",
    "backbone": "*"
  },
  "devDependencies": {
    "ratchet": "*",
    "bootstrap": "*"
  },
  "exportsOverride": {
    "requirejs-domready": {
      "": "domReady.js"
    },
    "requirejs-i18n": {
      "": "i18n.js"
    },
    "requirejs-text": {
      "": "text.js"
    },
    "ratchet": {
      "css": "dist/css/*",
      "fonts": "dist/fonts/*",
      "js": "dist/js/*"
    },
    "iscroll": {
      "": "build/*"
    }
  }
}
