{
  "name": "butterfly",
  "version": "0.2.0",
  "authors": [
    "Justin Yip <justin.yip@foxmail.com>"
  ],
  "description": "Butterfly Mobile Web Framework",
  "main": "dist/js/butterfly.js",
  "keywords": ["HTML5", "Mobile", "Web"],
  "license": "MIT",
  "homepage": "http://yezhiming.github.io/butterfly",
  "ignore": [
    "node_modules",
    "bower_components",
    "tests",
    "examples"
  ],
  "dependencies": {
    "requirejs": "*",
    "requirejs-text": "*",
    "requirejs-i18n": "*",
    "requirejs-domready": "*",
    "require-css": "*",
    "fastclick": "*",
    "iscroll": "*",
    "jquery": "*",
    "zepto": "*",
    "underscore": "*",
    "backbone": "*",
    "moment": "*",
    "spinjs": "*",
    "jquery-hammerjs": "*"
  },
  "devDependencies": {

  },

  "overrides": {
    "requirejs-domready": {
      "main": ["domReady.js"]
    },
    "jquery": {
      "main": ["dist/jquery.js"]
    },
    "fastclick": {
      "main": ["lib/fastclick.js"]
    },
    "iscroll": {
      "main": ["build/iscroll.js", "build/iscroll-probe.js"]
